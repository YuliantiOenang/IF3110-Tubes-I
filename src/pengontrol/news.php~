<?php
class News
{
	public function __construct()	
	{
		session_start();
	}

	public function main(array $getVars)
	{
		$news = new News_Model();
		$view = new View('news');
		$sql = new sql();
		
		$sql->connect();
		$sql->query("select * from kategori");
		while ($row = $sql->fetch())
		{
			echo $row->id.$row->name."<br>";
		}

		$view->setData('title' , 'asu');
		$view->setData('content' ,'asu');
		$view->render();
		$_SESSION['views']=1;
	}
}
